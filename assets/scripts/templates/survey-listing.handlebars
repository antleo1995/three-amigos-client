
<div class='survey-list' width='100%'>
  {{#each surveys as |survey|}}
  <div class='survey-{{survey.id}} survey'>
    <h1>{{survey.name}}<h1>
    <input type='button' value='Delete' id='survey-delete-{{survey.id}}' class='delete-survey'>
    <input type='button' value='Update' id='survey-update-{{survey.id}}' class='update-survey'>
  </div>
  <hr>
  {{/each}}
</div>
